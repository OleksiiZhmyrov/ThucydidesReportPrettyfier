{
  "name": "Thucydides report prettyfier",
  "version": "0.9",
  "description": "Improves reporting experience",
  "manifest_version": 2,
  "permissions": ["tabs", "http://*/*", "https://*/*", "file:///*/*"],
    "content_scripts": [
    {
        "matches": [ "http://*/*", "https://*/*", "file:///*/*"],
        "js": ["injector.js"],
        "all_frames": true
    }
  ],
  "web_accessible_resources": ["style_improvements.css"]
}